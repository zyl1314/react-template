{
  "extends": "eslint-config-airbnb",
  "parser": "babel-eslint",
  "globals": {
    "window": true,
    "document": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "arrow-parens": 0,
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "global-require": 0,
    "max-len": 0,
    "comma-dangle": 0,
    "class-methods-use-this": 0,
    "import/prefer-default-export": 0,
    "no-global-assign": 0,
    "no-unsafe-negation": 0,
    "no-console": 0,
    "eol-last": 0,
    "no-unused-vars": 0,
    "spaced-comment": 0,
    "no-param-reassign": 0,
    "react/no-deprecated": 0,
    "react/require-extension": "off",
    "no-lonely-if": 0,
    "func-names": 0,
    "new-cap": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "react/prop-types": 0,
    "import/no-extraneous-dependencies": 0,
    "jsx-a11y/no-static-element-interactions": "off",
    "linebreak-style": [
      "off",
      "windows"
    ]
  }
}
